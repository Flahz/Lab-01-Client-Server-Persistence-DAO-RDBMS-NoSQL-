						</action>
						<action>
							<loc>/tmp/wadiagnose.txt</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>/tmp/v4DebugInfo*</loc>
							<clear_log apply="false"/>
						</action>
					</file_copy_action>
					<use_extern_action>
