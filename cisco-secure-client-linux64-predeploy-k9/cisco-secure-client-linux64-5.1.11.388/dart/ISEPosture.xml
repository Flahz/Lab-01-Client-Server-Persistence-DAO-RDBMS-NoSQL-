						</action>
						<action>
							<loc>$HOME/.cisco/iseposture/log/csc_iseposture.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>/opt/cisco/secureclient/iseposture/csc_iseagent.log*</loc>
