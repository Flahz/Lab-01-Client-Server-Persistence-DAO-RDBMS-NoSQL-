		<file id="csc-ise-posture-logs" directoryName="Logs">
			<default>true</default>
			<description>ISE Posture application logs</description>
			<gui>
				<label>_tr("ISE Posture application logs")</label>
				<file_type>log</file_type>
			</gui>
			<dart>
				<required>false</required>
				<os opsys="win">
					<use_extern_action>
						<action>
							<plugin/>
								<args>wineventlog.exe -l "Cisco AnyConnect ISE Posture Module" -f %TEMP%\AnyConnect_ISEPosture.txt -o</args>
								<clear_log apply="false"/>
								<temp_out>%TEMP%\AnyConnect_ISEPosture.txt</temp_out>
						</action>
						<action>
							<path>%WINDIR%\System32</path>
							<args>wevtutil epl "Cisco AnyConnect ISE Posture Module" %TEMP%\AnyConnect_ISEPosture.evtx</args>
							<clear_log apply="true">wevtutil cl "Cisco AnyConnect ISE Posture Module"</clear_log>
							<temp_out>%TEMP%\AnyConnect_ISEPosture.evtx</temp_out>
						</action>
						<action>
							<plugin/>
                                <args>wineventlog.exe -l "Cisco Secure Client - ISE Posture" -f %TEMP%\ISEPosture.txt -o</args>
                                <clear_log apply="false"/>
                                <temp_out>%TEMP%\ISEPosture.txt</temp_out>
						</action>
						<action>
                            <path>%WINDIR%\System32</path>
							<args>wevtutil epl "Cisco Secure Client - ISE Posture" %TEMP%\CSC_ISEPosture.evtx</args>
							<clear_log apply="true">wevtutil cl "Cisco Secure Client - ISE Posture"</clear_log>
							<temp_out>%TEMP%\CSC_ISEPosture.evtx</temp_out>
						</action>
						<action>
							<path>%PROGRAMFILES%\Cisco\Cisco Secure Client\opswat</path>
							<args>WaDiagnose.exe --log_path "%LOCALAPPDATA%\Cisco\Cisco Secure Client\WaDiagnose.txt" --debug_log_output_path "%LOCALAPPDATA%\Cisco\Cisco Secure Client" --driver_prevent_install true --auto_exit true</args>
							<clear_log apply="false"/>
							<temp_out>%LOCALAPPDATA%\Cisco\Cisco Secure Client\WaDiagnose.txt</temp_out>
						</action>
				    </use_extern_action>
					<file_copy_action>
					    <action>
						    <loc>%LOCALAPPDATA%\Cisco\Cisco Secure Client\v4DebugInfo*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
						    <loc>%PROGRAMFILES%\Cisco\Cisco Secure Client\opswat\v4DebugInfo*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%LOCALAPPDATA%\Cisco\Cisco AnyConnect Secure Mobility Client\aciseposture.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%ALLUSERSPROFILE%\Cisco\Cisco AnyConnect Secure Mobility Client\Logs\elaciseposture.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%LOCALAPPDATA%\Cisco\Cisco AnyConnect Secure Mobility Client\DartAddon*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%ALLUSERSPROFILE%\Cisco\Cisco AnyConnect Secure Mobility Client\ISE Posture\aciseagent.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%LOCALAPPDATA%\Cisco\Cisco Secure Client\csc_iseposture.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%ALLUSERSPROFILE%\Cisco\Cisco Secure Client\Logs\csc_eliseposture.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%ALLUSERSPROFILE%\Cisco\Cisco Secure Client\ISE Posture\csc_iseagent.log*</loc>
							<clear_log apply="false"/>
						</action>
						<action>
							<loc>%LOCALAPPDATA%\Cisco\Cisco Secure Client\DartAddon*</loc>
							<clear_log apply="false"/>
						</action>
					</file_copy_action>
					<use_extern_action>
					    <action>
						    <path>%WINDIR%\System32</path>
							<args>cmd.exe /C "del "%LOCALAPPDATA%\Cisco\Cisco Secure Client\v4DebugInfo*""</args>
					    </action>
					    <action>
						    <path>%WINDIR%\System32</path>
							<args>cmd.exe /C "del "%PROGRAMFILES%\Cisco\Cisco Secure Client\opswat\v4DebugInfo*""</args>
					    </action>
					</use_extern_action>
				</os>
				<os opsys="linux">
					<use_extern_action>
						<action>
							<path>/opt/cisco/secureclient/lib/opswat</path>
							<args>wadiagnose --log_path /tmp/wadiagnose.txt --debug_log_output_path /tmp --driver_prevent_install true --auto_exit true</args>
							<clear_log apply="false"/>
							<stdout/>
						</action>
					</use_extern_action>
					<file_copy_action>
						<action>
							<loc>$HOME/.cisco/iseposture/log/aciseposture.log*</loc>
